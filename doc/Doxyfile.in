PROJECT_NAME		= "@CMAKE_PROJECT_NAME@"
OUTPUT_DIRECTORY	= @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT			    = @CMAKE_SOURCE_DIR@/src
GENERATE_LATEX		= NO
EXTRACT_ALL		    = YES
EXTRACT_PRIVATE		= NO
RECURSIVE		    = YES
FULL_PATH_NAMES		= NO
CLASS_DIAGRAMS      = YES
HIDE_UNDOC_RELATIONS = NO
HAVE_DOT             = YES
CLASS_GRAPH          = YES
COLLABORATION_GRAPH  = YES
UML_LOOK             = YES
UML_LIMIT_NUM_FIELDS = 50
TEMPLATE_RELATIONS   = YES
DOT_GRAPH_MAX_NODES  = 100
MAX_DOT_GRAPH_DEPTH  = 0
DOT_TRANSPARENT      = YES
TAGFILES            += "@CMAKE_CURRENT_SOURCE_DIR@/cppreference-doxygen-web.tag.xml=http://en.cppreference.com/w/"
